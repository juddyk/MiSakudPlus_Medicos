<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.appdemo.mi_salud.misakudplus_medicos.UI_registro.registroIX">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="5sp"
            android:layout_marginStart="5sp"
            android:layout_marginTop="10sp"
            android:orientation="vertical">

            <TextView
                style="@style/txtSub0"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/reg_mensajeUsuario" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/regDirPAC"
                    style="@style/txtBox3"
                    android:layout_width="0sp"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="5sp"
                    android:layout_marginStart="10sp"
                    android:layout_weight="2"
                    android:background="@drawable/round_txt"
                    android:gravity="center"
                    android:hint="@string/reg_sede"
                    android:inputType="textMultiLine"
                    android:maxLines="5"
                    android:padding="5dp"
                    android:scaleType="fitCenter" />

                <ImageButton
                    android:id="@+id/btnDirPAC"
                    android:layout_width="35sp"
                    android:layout_height="35sp"
                    android:layout_marginEnd="15sp"
                    android:layout_marginStart="5sp"
                    android:background="@drawable/img_btn"
                    android:contentDescription="@string/reg_direccion"
                    android:padding="5sp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/editar" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/regHourPAC"
                    style="@style/txtBox3"
                    android:layout_width="0sp"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="5sp"
                    android:layout_marginStart="10sp"
                    android:layout_weight="2"
                    android:background="@drawable/round_txt"
                    android:gravity="center"
                    android:hint="@string/reg_horarioConsulta"
                    android:padding="5dp"
                    android:scaleType="fitCenter" />

                <ImageButton
                    android:id="@+id/btnHourPAC"
                    android:layout_width="35sp"
                    android:layout_height="35sp"
                    android:layout_marginEnd="15sp"
                    android:layout_marginStart="5sp"
                    android:background="@drawable/img_btn"
                    android:contentDescription="@string/app_registro"
                    android:padding="5sp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/editar" />

            </LinearLayout>

            <!-- TIPO DE CONSULTA -->

            <TextView
                style="@style/txtSub"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10sp"
                android:text="@string/reg_tipoConsulta" />
            <TextView
                style="@style/txtSub0"
                android:layout_width="match_parent"
                android:layout_height="18sp"
                android:text="@string/reg_mensajeAtencion"
                android:gravity="center"
                />

            <CheckBox
                android:id="@+id/consulta1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reg_consulta1"
                android:layout_marginLeft="10sp"
                android:layout_weight="1"
                android:layout_marginRight="10sp"
                style="@style/txtSub"/>

            <CheckBox
                android:id="@+id/consulta2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reg_consulta2"
                android:layout_marginLeft="10sp"
                android:layout_weight="1"
                android:layout_marginRight="10sp"
                style="@style/txtSub"/>
            <CheckBox
                android:id="@+id/consulta3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reg_consulta3"
                android:layout_marginLeft="10sp"
                android:layout_weight="1"
                android:layout_marginRight="10sp"
                style="@style/txtSub"/>

            <CheckBox
                android:id="@+id/consulta4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reg_consulta4"
                android:layout_marginLeft="10sp"
                android:layout_weight="1"
                android:layout_marginRight="10sp"
                style="@style/txtSub"/>

            <CheckBox
                android:id="@+id/consulta5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reg_consulta5"
                android:layout_marginLeft="10sp"
                android:layout_weight="1"
                android:layout_marginRight="10sp"
                style="@style/txtSub"/>

            <CheckBox
                android:id="@+id/consulta6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reg_consulta6"
                android:layout_marginLeft="10sp"
                android:layout_weight="1"
                android:layout_marginRight="10sp"
                style="@style/txtSub"/>




            <!-- MODALIDAD DE ATENCION -->

            <TextView
                style="@style/txtSub"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10sp"
                android:text="@string/reg_modalidad" />
            <TextView
                style="@style/txtSub0"
                android:layout_width="match_parent"
                android:layout_height="18sp"
                android:text="@string/reg_mensajeAtencion"
                android:gravity="center"
                />

            <CheckBox
                android:id="@+id/atencion1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reg_atencion1"
                android:layout_marginLeft="10sp"
                android:layout_weight="1"
                android:layout_marginRight="10sp"
                style="@style/txtSub"/>

            <CheckBox
                android:id="@+id/atencion2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reg_atencion2"
                android:layout_marginLeft="10sp"
                android:layout_weight="1"
                android:layout_marginRight="10sp"
                style="@style/txtSub"/>
            <CheckBox
                android:id="@+id/atencion3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reg_atencion3"
                android:layout_marginLeft="10sp"
                android:layout_weight="1"
                android:layout_marginRight="10sp"
                style="@style/txtSub"/>

            <CheckBox
                android:id="@+id/atencion4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reg_atencion4"
                android:layout_marginLeft="10sp"
                android:layout_weight="1"
                android:layout_marginRight="10sp"
                style="@style/txtSub"/>

            <CheckBox
                android:id="@+id/atencion5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reg_atencion5"
                android:layout_marginLeft="10sp"
                android:layout_weight="1"
                android:layout_marginRight="10sp"
                style="@style/txtSub"/>

            <CheckBox
                android:id="@+id/atencion6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reg_atencion6"
                android:layout_marginLeft="10sp"
                android:layout_weight="1"
                android:layout_marginRight="10sp"
                style="@style/txtSub"/>
            <CheckBox
                android:id="@+id/atencion7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reg_atencion7"
                android:layout_marginLeft="10sp"
                android:layout_weight="1"
                android:layout_marginRight="10sp"
                style="@style/txtSub"/>

            <CheckBox
                android:id="@+id/atencion8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reg_atencion8"
                android:layout_marginLeft="10sp"
                android:layout_weight="1"
                android:layout_marginRight="10sp"
                style="@style/txtSub"/>

            <TextView
                android:layout_marginTop="50sp"
                android:gravity="end"
                android:layout_marginEnd="10sp"
                android:layout_marginBottom="20sp"
                android:id="@+id/idNext"
                style="@style/txtSub0"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="@string/reg_next"
                android:visibility="visible" />
        </LinearLayout>


    </ScrollView>

</FrameLayout>
